@model IEnumerable<Part5.Models.StringHolder>

@{
    ViewBag.Title = "Convert";
}

<h2>Convert</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Lowercase)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Uppercase)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Inverted)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Lowercase)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Uppercase)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Inverted)
        </td>
        
        
    </tr>
}

</table>
